    "next": "10.0.3",
    "next-seo": "4.17.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@svgr/webpack": "5.5.0",
    "@types/gtag.js": "0.0.3",
    "@types/marked": "1.2.1",
    "@types/node": "14.14.10",
    "@types/react": "17.0.0",
    "@types/remove-markdown": "0.1.1",
    "@types/rss": "0.0.28",
    "autoprefixer": "10.0.4",
    "canvas": "2.6.1",
    "cssnano": "4.1.10",
    "husky": "4.3.0",
    "lint-staged": "10.5.2",
    "next-secure-headers": "2.0.0",
    "postcss": "8.1.13",
    "postcss-flexbugs-fixes": "5.0.2",
