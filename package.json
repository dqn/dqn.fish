    "textlint": "11.7.6",
    "textlint-rule-preset-ja-spacing": "2.0.2",
    "textlint-rule-preset-ja-technical-writing": "4.0.1",
    "textlint-rule-spellcheck-tech-word": "5.0.0",
    "ts-node": "9.1.0",
    "typescript": "4.1.2"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
